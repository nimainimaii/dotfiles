# make sure $userclientrc is set to $HOME/.config/X11/xinitrc in /usr/bin/startx script

xmodmap ~/.config/X11/Xmodmap &

xset r rate 300 50 &

xgamma -rgamma 1.0 -ggamma 0.9 -bgamma 0.7 &

mpd &

picom &

xbanish &

dwmblocks &

(dmenu-change-mode "gruvbox_dark"

set-wallpaper ~/pictures/wallpapers

dmenu-change-font "Liberation Mono") &

exec dwm
