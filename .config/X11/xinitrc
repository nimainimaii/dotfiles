# make sure $userclientrc is set to $HOME/.config/X11/xinitrc in /usr/bin/startx script

xmodmap ~/.config/X11/Xmodmap &

xset r rate 300 50 &

redshift -P -O 2700 &

killall mpd ; mpd &

(dmenu-change-font "Liberation Mono" ; dmenu-change-mode "gruvbox_dark") &

set-wallpaper ~/pictures/wallpapers &

picom &

killall xbanish ; xbanish &

killall flameshot ; flameshot &

killall dwmblocks ; dwmblocks &

exec dwm
